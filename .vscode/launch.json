{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "g++ - Build and Debug Disruptor",
        "type": "cppdbg",               
        "request": "launch",            
        "program": "${workspaceFolder}/bin/unicast_1P-1C",  
        "stopAtEntry": false,           
        "cwd": "${workspaceFolder}",    
        "environment": [],              
        "externalConsole": false,       
        "MIMode": "gdb",                
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "build_disruptor_test",        
        "miDebuggerPath": "/usr/bin/gdb"          
      }
    ]
  }